<!DOCTYPE html>
<html>
  <head>
    <title>Backoffice</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'backoffice', media: 'all', 'data-turbolinks-track': 'reload' %>

    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'backoffice', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

    <div id="wrapper">
      <%= render partial: "layouts/backoffice_shared/navigation" %>
      <%= yield %>
    </div>

</body>

</html>
